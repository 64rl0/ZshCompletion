# base_completion
# vim: ft=sh

#   __|    \    _ \  |      _ \   __| __ __| __ __|
#  (      _ \     /  |     (   | (_ |    |      |
# \___| _/  _\ _|_\ ____| \___/ \___|   _|     _|


for completion in "${HOME}/.zsh/completion/"*
do

    if [[ "$(basename ${completion})" == "base_completion" || "$(basename ${completion})" == "README.md" ]]; then
        continue
    fi

    [[ -r "${completion}" ]] && source "${completion}"

done

